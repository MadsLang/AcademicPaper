	year = {2019},
	doi = {10.1007/978-3-030-17906-9_2},
	keywords = {Social media, Journalism, Intermediary, News sharing, Commendary culture, Cultural politics},
	pages = {21--60},
	file = {Full Text PDF:/home/gaardhus/Zotero/storage/GLHRFAJ5/Martin - 2019 - Commendary Cultures.pdf:application/pdf},
}

